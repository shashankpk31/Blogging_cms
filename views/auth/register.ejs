<% include ../partials/header.ejs %>

<div class="container center-align">
  <div class="loginSignup-container">
  
    <form action="/register" method="POST" id="form">
      <h2>Signup</h2>
      <label for="username" id="label"></label>
      <input type="text" name="username" placeholder="Username" id="input"/>
      <label for="password" id="label"></label>
      <input type="password" name="password" placeholder="Password" id="input" />
      <label for="email" id="label"></label>
      <input type="text" name="email" placeholder="email" id="input" />
      <label for="Age" id="label"></label>
      <input type="number" name="Age" placeholder="Age" id="input" />
      <label for="entity" id="label"></label>
      <input type="text" name="entity" placeholder="entity" id="input" />
      <label for="image" id="label"></label>
      <input type="text" name="image" placeholder="image link" id="input" />
      <button class="btn">Submit</button>
    </form>
  </div>
</div>

<% include ../partials/footer.ejs %>
